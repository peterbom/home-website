{
  "scripts": {
    "init": "shx cp -n .env.template .env",
    "preclean": "npm run unbundle",
    "clean": "rimraf dist/* && mkdirp dist",
    "prebuild": "npm run clean",
    "build:system": "zoo babel src --out-dir dist",
    "build:html": "html-minifier --collapse-whitespace --input-dir src --file-ext html --output-dir dist",
    "build:css": "cleancss --output dist/styles.css < styles/styles.css",
    "build": "concurrently \"npm run build:system\" \"npm run build:html\" \"npm run build:css\"",
    "bundle": "zoo node tasks/bundle.js",
    "unbundle": "zoo node tasks/unbundle.js",
    "test": "karma start --single-run",
    "test:tdd": "karma start",
    "test:tdd-debug": "node-debug node_modules/karma/bin/karma start",
    "test:cover": "echo \"Error: no test specified\"",
    "export:clean": "rimraf export/* && mkdirp export",
    "export:normalized-resources": "zoo node tasks/export-normalized-resources.js",
    "export:copy": "zoo node tasks/export-copy.js",
    "export": "npm run build && npm run bundle && npm run export:clean && npm run export:normalized-resources && npm run export:copy",
    "serve": "zoo jspm-server . --no-browser",
    "serve:export": "zoo NODE_ENV=production npm run export && jspm-server ./export --no-browser",
    "watch:src": "watch \"npm run build && npm run test\" src",
    "watch:css": "watch \"npm run build:css\" styles",
    "watch": "concurrently \"npm run watch:src\" \"npm run watch:css\" \"npm run test:tdd\" \"npm run serve\""
  },
  "name": "aurelia-home-website",
  "version": "1.0.0",
  "description": "A site to provide access to home management APIs",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "ssh://gituser@192.168.1.225:60022/volume1/code/git/aurelia-home-website.git"
  },
