{
  "scripts": {
    "init-env": "shx cp -n .env.template .env",
    "unbundle": "node tasks/unbundle.js",
    "clean-debug": "npm run unbundle && rimraf dist/* && mkdirp dist",
    "clean-release": "npm run unbundle && rimraf export/* && mkdirp export/dist",
    "build:env-dev": "zoo NODE_ENV=development node tasks/set-env.js dist/env.js",
    "build:env-prod": "zoo NODE_ENV=production node tasks/set-env.js dist/env.js",
    "build:system": "zoo babel src --out-dir dist",
    "build:html": "html-minifier --collapse-whitespace --input-dir src --file-ext html --output-dir dist",
    "build:css": "cleancss --output dist/styles.css < assets/styles/styles.css",
    "build:export": "zoo node tasks/bundle.js && zoo node tasks/export-normalized-resources.js && zoo node tasks/export-copy.js",
    "build:dev-debug": "concurrently \"npm run build:env-dev\" \"npm run build:system\" \"npm run build:html\" \"npm run build:css\"",
    "build:prod-debug": "concurrently \"npm run build:env-prod\" \"npm run build:system\" \"npm run build:html\" \"npm run build:css\"",
    "build:dev-release": "npm run build:dev-debug && npm run build:export",
    "build:prod-release": "npm run build:prod-debug && npm run build:export",
    "build-dev-debug": "npm run clean-debug && npm run build:dev-debug",
    "build-prod-debug": "npm run clean-debug && npm run build:prod-debug",
    "build-dev-release": "npm run clean-release && npm run build:dev-release",
    "build-prod-release": "npm run clean-release && npm run build:prod-release",
    "test": "karma start karma-conf/test.js",
    "test:tdd": "karma start karma-conf/tdd.js",
    "test:tdd-debug": "node-debug karma start karma-conf/tdd.js",
    "test:cover": "karma start karma-conf/cover.js",
    "test:cover-debug": "node-debug karma start karma-conf/cover.js",
    "serve-debug": "jspm-server . --no-browser",
    "serve-release": "jspm-server ./export --no-browser",
    "watch:src": "watch \"npm run build-dev-debug\" src",
    "watch:css": "watch \"npm run build:css\" assets/styles",
    "watch": "concurrently \"npm run watch:src\" \"npm run watch:css\" \"npm run test:tdd\" \"npm run serve-debug\""
  },
